    "doctrine/doctrine-bundle": "~1.2",
    "friendsofphp/php-cs-fixer": "^2.6",
    "leofeyer/optimize-native-functions-fixer": "^1.1",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message-factory": "^1.0.2",
    "phpunit/phpunit": "^5.7.26",
    "symfony/phpunit-bridge": "^3.2"
  },
  "extra": {
    "contao-manager-plugin": "Pdir\\MobileDeBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Pdir\\MobileDeBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pdir\\MobileDeBundle\\Tests\\": "tests/"
    }
  },
  "support": {
    "docs": "https://docs.pdir.de/mobilede/mobilede_inserate.html",
