  "keywords": [
    "contao",
    "mobile",
    "ads",
    "cars",
    "automobile",
    "car",
    "Fahrzeuge",
    "Wohnmobile",
    "LKW",
    "Trucks"
  ],
  "license": "proprietary",
  "homepage": "https://pdir.de/mobile-de-integration-fuer-contao-cms.html",
  "authors": [
    {
      "name": "Webdesign f√ºr Dresden",
      "homepage": "https://pdir.de",
      "email": "develop@pdir.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "^4.4.8"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
